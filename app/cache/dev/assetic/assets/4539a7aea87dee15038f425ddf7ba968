function muestraCuentaAtras(h,d){var c,e,g;var a=new Date();var f=new Date(d.ano,d.mes,d.dia,d.hora,d.minuto,d.segundo);var b=Math.floor((f.getTime()-a.getTime())/1000);if(b<0){cuentaAtras="-"}else{c=Math.floor(b/3600);b=b%3600;e=Math.floor(b/60);b=b%60;g=Math.floor(b);cuentaAtras=(c<10?"0"+c:c)+"h "+(e<10?"0"+e:e)+"m "+(g<10?"0"+g:g)+"s ";setTimeout(function(){muestraCuentaAtras(h,d)},1000)}document.getElementById(h).innerHTML=cuentaAtras};